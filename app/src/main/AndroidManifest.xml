<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anatoly.tcat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.anatoly.tcat.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.anatoly.tcat.DictionaryViewActivity" />
        <activity android:name="com.anatoly.tcat.AddEntryActivity"/>
        <activity android:name="com.anatoly.tcat.WritingTermExerciseActivity"/>
    </application>

</manifest>
